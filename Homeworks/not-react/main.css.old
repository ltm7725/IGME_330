#hsv-map {
	opacity: 0;
    z-index: 1;
	width: 100%;
	height: 100%;
	position: absolute;
	top: -8vw;
	left: 0vw;
}
#surface {
	position: absolute;
	z-index: 0;
    color: white;
	width: 100%;
	top: 0;
}
#hsv-map .cover {
    z-index: 2;
    width: 31.6vw; 
    height: 31.6vw; 
	opacity: 0;
	background-color: #000;
	border-radius: 50%;
	cursor: crosshair;
}
.bar-bg, .bar-white, #luminance-bar, .hsv-barcursors{
    position: absolute;
}
.bar-bg, .bar-white, #luminance-bar, .hsv-barcursors, #hsv-map .bar-bg, #hsv-map .bar-white{
	position: absolute;
	width: 9.6vw; 
	height: 31.6vw; 
	top: 0;
	left: 33.5vw; 
}
#hsv-map .bar-white {
	background-color: #fff;
}
.hsv-cursor {
	z-index: 1;
	position: absolute;
	border: 1px solid rgb(255, 255, 255);
	border-radius: 50%;
	width: 1vw;
	height: 1vw;
	cursor: default;
	margin: -5px;
	cursor: crosshair;
}
.dark .hsv-cursor {
	border-color: rgb(0, 0, 0);
}
.no-cursor .hsv-cursor, #hsv-map.no-cursor .cover {
	cursor: none; /* also works in IE8 */
	/*url(_blank.gif), url(_blank.png), url(_blank.cur), auto;*/
}
#luminance-bar {
	position: absolute;
	right: -1vw;
	top: 0;
	background: url("../media/tp.png");
	background-size: 100% 100%;
	/* background-color: red; */
}
.hsv-barcursors {
	position: absolute;
	right: -1vw;
	top: 0;
	overflow: hidden;
}
.hsv-barcursor-l, .hsv-barcursor-r {
	position: absolute;
	border: .5vw solid transparent; 
	margin-top: -4px;
}
.hsv-barcursor-l {
	left: 0;
	border-left: 4px solid #eee;
}
.hsv-barcursor-r {
	right: 0;
	border-right: 4px solid #eee;
}
.dark .hsv-barcursor-l {
	border-left-color: rgb(255, 255, 255);
}
.dark .hsv-barcursor-r {
	border-right-color: rgb(255, 255, 255);
}

